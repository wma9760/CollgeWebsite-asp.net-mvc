@model IEnumerable<CollgeWebsite.Models.Department>

@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_DashboardLayout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Dp_Code)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dp_Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dp_Description)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Dp_Image)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Dp_Code)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dp_Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dp_Description)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Dp_Image)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
